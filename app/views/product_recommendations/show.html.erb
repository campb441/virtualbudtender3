<div class="page-header">
  <h3>Product Recommendation #<%= @product_recommendation.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @product_recommendation.user_id %></dd>

      <dt>Budtender</dt>
      <dd><%= @product_recommendation.budtender_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/product_recommendations" class="btn btn-primary">
        Back
      </a>
      <a href="/product_recommendations/<%= @product_recommendation.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_product_recommendation/<%= @product_recommendation.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
